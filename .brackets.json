{
    "sass.enabled": false,
    "path": {
        "scss/*.scss": {
            "sass.enabled": true,
            "sass.options": {
                "outputDir": "../css/",
                "includePaths": [],
                "sourceComments": false,
                "outputStyle": "compressed"
            },
            "linting.collapsed": true,
            "useTabChar": true,
            "fonts.fontSize": "12px"
        }
    },
    "brackets-latex.texBinDirectory": "/Library/TeX/texbin",
    "brackets-latex.outputDirectory": "out",
    "brackets-latex.compiler": "pdflatex",
    "brackets-latex.mainFile": null
}